{
  "name": "ULTRA X ASSISTANT",
  "description": "Open-Source ASSISTANT Bot for Telegram.",
  "keywords": ["Telegram", "Ultra x", "assistant"],
  "repository": "https://github.com/ULTRA-OP/ULTRA-X",
  "stack": "container",
  "env": {
    "TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
    },
}
